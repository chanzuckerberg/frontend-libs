{
  "compilerOptions": {
    "allowJs": false,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "lib": ["es2019"],
    "module": "commonjs",
    "target": "es5",
    "skipLibCheck": true,
    "strict": true
  }
}
