{
  "compilerOptions": {
    "allowJs": false,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["dom", "es2019"],
    "module": "es6",
    "moduleResolution": "node",
    "target": "es2019",
    "skipLibCheck": true,
    "strict": true
  }
}
